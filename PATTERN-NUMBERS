#include<stdio.h>
void main()
{
    int row,column,c=1;
    printf("Enter the number of rows and columns:");
    scanf("%d%d",&row,&column);
    for(int i=1;i<=row;i++)
    {
        for(int j=1;j<=column;j++)
        {
            printf("     %d",c);
            c+=1;
        }
        printf("\n");
    }
}
